{
  "name": "propertycross",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build": "webpack --mode production",
    "test": "jest",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "prettier": "prettier  â€” write src/**/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:write",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "clearMocks": false,
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(node_modules|.vscode)/"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexAnoschenko/PropertyCross.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlexAnoschenko/PropertyCross/issues"
  },
  "homepage": "https://github.com/AlexAnoschenko/PropertyCross#readme",
